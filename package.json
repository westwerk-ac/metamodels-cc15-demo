{
    "name": "contao-gulp-boilerplate",
    "description": "A starting point for Contao theme development with Gulp and Bower",
    "version": "1.0.0",
    "homepage": "https://github.com/westwerk-ac/contao-gulp-boilerplate",
    "repository": "github.com/westwerk-ac/contao-gulp-boilerplate",
    "dependencies": {
        "amd-optimize": "*",
        "gulp": "^3.8.8",
        "gulp-autoprefixer": "^1.0.1",
        "gulp-concat": "^2.4.1",
        "gulp-header": "^1.1.1",
        "gulp-jshint": "^1.8.5",
        "gulp-less": "^1.3.6",
        "gulp-livereload": "^2.1.1",
        "gulp-minify-css": "^0.3.10",
        "gulp-notify": "^2.0.0",
        "gulp-plumber": "^0.6.6",
        "gulp-sourcemaps": "^1.2.4",
        "gulp-watch": "^1.1.0",
        "jshint-stylish": "^1.0.0",
        "map-stream": "^0.1.0",
        "require-dir": "^0.1.0",
